{
  "manifest_version": 2,
  "name": "Times New Roman Everything",
  "description": "Extension that turns all website font to Times New Roman",
  "version": "2.1.5",
  "homepage_url": "https://github.com/NicknamesAreNotHard/New-Times-Roman-Everything",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/16.png",
    "default_title": "Change Times New Roman Everything settings",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["background.js"]
  }]
}
